
<html>

<head>

	<% include head.ejs %>
	
</head>

<body class="skin-blue sidebar-mini">
<div class="wrapper">

	<% include header.ejs %>
  
	
	<aside class="main-sidebar">

    
		<section class="sidebar">
			
				<ul class="sidebar-menu">
				<li class="header">SIGN IN/CREATE AN ACCOUNT</li>
				<li class="" ><a href="/login" class="fa fa-laptop"></i> <span>Log In</span></a></li>
				<li class="" ><a href="/register"><i class="fa fa-edit"></i> <span>Create an Account</span></a></li>			
      

				</ul>
			

		</section>
    

	</aside>

	
	<div class="content-wrapper">

	
		<section class="content-header">
			<h1>
				SIGN IN
				
			<small></small>
			</h1>
      
		</section>

   
    <section class="content">
		<div class="box box-info">
			<div class="box-header with-border">
				<h3 class="box-title">Sign in to your account </h3>
			</div>
            
			<div class="container">
				<form action="/login" method="POST">
					<div class="form-group">
					  <label for="username">Username:</label>
					 <input type="text" name="username" placeholder="Username" class="form-control">
					</div>
					<div class="form-group">
					  <label for="pwd">Password:</label>
					  <input type="password" name="password" placeholder="Password" class="form-control">
					</div>
					<button type="submit" class="btn btn-default">Submit</button>
				</form>
			</div>
          </div>
		</div>
   </section>
</div>

  
  <% include footer.ejs %> 

</div>
<!-- ./wrapper -->

